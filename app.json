  "env": {
    "DEV": {
      "description": "Put any phone numbers you want to be bot owner without + or spaces \",\" Ex: 254114660061,27849061633",
      "value": "254102562476",
      "required": true
    },
    "HEROKU_API": {
      "description": "Put a Heroku API key here, will be required to update vars in your DM",
      "required": true
